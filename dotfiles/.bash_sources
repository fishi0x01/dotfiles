#!/bin/bash

[[ -s ~/.git-completion.sh ]] && source ~/.git-completion.sh
[[ -s ~/.pass-completion.sh ]] && source ~/.pass-completion.sh
[[ -s ~/.rbenv/completions/rbenv.bash ]] && source ~/.rbenv/completions/rbenv.bash
[[ -s ~/.gvm/scripts/gvm ]] && source ~/.gvm/scripts/gvm
[[ -s ~/.rbenv/bin/rbenv ]] && eval "$(rbenv init -)"
